{
  "name": "app-cards",
  "version": "1.1.0",
  "description": "Server-side Rendering React from Scratch!",
  "main": "index.js",
  "engines": {
    "node": "8.10.0",
    "npm": "5.7.1"
  },
  "scripts": {
    "clean": "rm -rf views/ 2> /dev/null ; rm -rf v8-compile-cache-0/ 2> /dev/null ; rm assets/bundle.js 2> /dev/null ; rm assets/client.js 2> /dev/null",
    "compile": "npm run pack && npm run babel",
    "pack": "webpack --mode development --config webpack.config.js --progress",
    "babel": "babel src -d views",

    "build": "npm run clean && npm run compile",
    "start": "npm run build && node index.js",
    "watch": "npm run build && nodemon --delay 0.33 --watch src --exec npm run start"
  },
  "author": "Rohit kumar",
  "license": "CC BY 4.0",
  "dependencies": {
    "express": "^4.16.4",
    "nodemon": "^1.18.11",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^5.1.1",
    "redux": "^3.7.2",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1"
  }
}
